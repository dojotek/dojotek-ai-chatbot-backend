{
  "name": "dojotek-ai-chatbot-backend",
  "version": "0.0.1",
  "description": "Dojotek AI Chatbot is a software system to help enterprise/company/corporate to build, configure, run, monitor multiple Chatbot AI LLM RAG.",
  "author": "Andy Primawan",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --ignore-pattern \"src/generated/**/*\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:roles": "ts-node -r tsconfig-paths/register src/scripts/seed-roles.ts",
    "seed:users": "ts-node -r tsconfig-paths/register src/scripts/seed-users.ts",
    "seed:all": "ts-node -r tsconfig-paths/register src/scripts/seed-all.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@langchain/anthropic": "^0.3.28",
    "@langchain/community": "^0.3.56",
    "@langchain/core": "^0.3.76",
    "@langchain/openai": "^0.6.12",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.0",
    "@opentelemetry/context-async-hooks": "^2.1.0",
    "@opentelemetry/core": "^2.1.0",
    "@opentelemetry/exporter-jaeger": "^2.1.0",
    "@opentelemetry/exporter-prometheus": "^0.205.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.51.0",
    "@opentelemetry/instrumentation-pino": "^0.52.0",
    "@opentelemetry/instrumentation-runtime-node": "^0.19.0",
    "@opentelemetry/propagator-b3": "^2.1.0",
    "@opentelemetry/propagator-jaeger": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ioredis": "^5.7.0",
    "langchain": "^0.3.34",
    "nestjs-otel": "^7.0.1",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/generated/",
        "module",
        "main.ts",
        "tracing.ts",
        "logs.service.ts",
        "/scripts/",
        "/auth/guards/",
        "/auth/strategies/",
        "prisma.service"
      ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
